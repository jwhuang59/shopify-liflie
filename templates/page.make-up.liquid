$(".nurse-content .part2-3 .video-box .btn-right").click(function(){
  index++;
  if(index > 6/num-1){
    index = 6/num-1
    $(".nurse-content .part2-3 .videopage ul").animate({"left":-part_W*index-150},300,function(){
      $(this).animate({"left":-part_W*index},300)

    })

  }
  $(".nurse-content .part2-3 .videopage ol li").eq(index).addClass("active").siblings().removeClass("active")
  $(".nurse-content .part2-3 .videopage ul").animate({"left":-part_W*index},500)

})

$(".nurse-content .part2-3 .video-box .btn-left").click(function(){
  index--;
  if(index < 0){
    index = 0
    $(".nurse-content .part2-3 .videopage ul").animate({"left":"150px"},300,function(){
      $(this).animate({"left":0},300)

    })

  }
  $(".nurse-content .part2-3 .videopage ol li").eq(index).addClass("active").siblings().removeClass("active")
  $(".nurse-content .part2-3 .videopage ul").animate({"left":-part_W*index},500)

})



