  index = $(this).index();
  
  $(".nurse-content .part2-3 .videopage ul").animate({"left":-part_W*index},500,function(){
    $(".nurse-content .part2-3 .videopage ol li").eq(index).addClass("active").siblings().removeClass("active")
    $(".nurse-content .part2-3 .wrapper-video .video-box").eq(index).stop().fadeIn(500).siblings().fadeOut(500)
